          "_name": "restaurant-menu2",
          "_customHTML": "<section class=\"restaurant-menu2 restaurant-special-menu\" group=\"Restaurant Menu\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"mbr-restaurant-special-menu\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Oben\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Unten\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Titel Anzeigen\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Unterstrich Anzeigen\" name=\"showUnderline\">\n        <input type=\"color\" title=\"Unterstrichfarbe\" name=\"underlineColor\" value=\"#ffbc00\" condition=\"showUnderline\">\n\n        <input type=\"color\" title=\"Sonderangebot\" name=\"specialColor\" value=\"#9f3942\">\n        <select title=\"Gruppen\" name=\"groups\">\n             <option value=\"2\">2</option>\n             <option value=\"3\">3</option>\n             <option value=\"4\">4</option>\n             <option value=\"5\" selected>5</option>\n             <option value=\"6\">6</option>\n        </select>\n        <input type=\"range\" title=\"Gruppe1 Speisen\" inline name=\"group1Items\" min=\"1\" max=\"50\" value=\"6\" step=\"1\">\n        <input type=\"text\" title=\"Spezialitäten in der Gruppe\" name=\"specialsInGroup1\" value>\n\n        <input type=\"range\" title=\"Gruppe2 Speisen\" inline name=\"group2Items\" min=\"1\" max=\"50\" value=\"7\" step=\"1\">\n        <input type=\"text\" title=\"Spezialitäten in der Gruppe\" name=\"specialsInGroup2\" value>\n\n        <input type=\"range\" title=\"Gruppe3 Speisen\" inline name=\"group3Items\" min=\"1\" max=\"50\" value=\"2\" step=\"1\" condition=\"groups >2\">\n        <input type=\"text\" title=\"Spezialitäten in der Gruppe\" name=\"specialsInGroup3\" value condition=\"groups>2\">\n\n        <input type=\"range\" title=\"Gruppe4 Speisen\" inline name=\"group4Items\" min=\"1\" max=\"50\" value=\"5\" step=\"1\" condition=\"groups >3\">\n        <input type=\"text\" title=\"Spezialitäten in der Gruppe\" name=\"specialsInGroup4\" value condition=\"groups>3\">\n\n        <input type=\"range\" title=\"Gruppe5 Speisen\" inline name=\"group5Items\" min=\"1\" max=\"50\" value=\"2\" step=\"1\" condition=\"groups >4\">\n        <input type=\"text\" title=\"Spezialitäten in der Gruppe\" name=\"specialsInGroup5\" value condition=\"groups>4\">\n\n        <input type=\"range\" title=\"Gruppe6 Speisen\" inline name=\"group6Items\" min=\"1\" max=\"50\" value=\"1\" step=\"1\" condition=\"groups >5\">\n        <input type=\"text\" title=\"Spezialitäten in der Gruppe\" name=\"specialsInGroup6\" value condition=\"groups>5\">\n\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" value=\"@PROJECT_PATH@/assets/images/background4.jpg\" title=\"Hintergrundbild\" selected>\n            <input type=\"color\" value=\"#ffffff\" title=\"Hintergrundfarbe\">\n            <input type=\"video\" value=\"https://www.youtube.com/watch?v=pjiVuLVqWQg\" title=\"Hintergrundvideo\">\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay-Farbe\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Transparenz\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <h2 class=\"mbr-section-title align-center mbr-fonts-style mbr-black\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title,.ornament\">\n            Erforschen Sie Unser Menü\n        </h2>\n        <div class=\"py-3 ornament align-center\" mbr-if=\"showUnderline\">\n            <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 133 20\" height=\"20\" width=\"150\" fill=\"{{underlineColor}}\">\n                <g><path d=\"M127.18,6.19C121-.05,109.94-.24,101.84.1,90.84.56,80,3.3,69.32,5.77c-8,1.86-16,3.63-24,5.72-9.21,2.41-18.6,4.72-28.14,5.19-4.1.2-8.48,0-12.16-2a6.6,6.6,0,0,1-2.7-9A5.46,5.46,0,0,1,7.07,2.77c4-.22,6.73,8.39-.08,4.58a.49.49,0,0,0-.49.84c4.88,3,10.8,0,5.4-4.91C7.6-.63.4,2.06,0,8-1,24,28.25,17,36.29,15.24,53.74,11.37,70.9,6.44,88.53,3.33c11.1-2,24.75-3.54,35.06,2.19a16,16,0,0,1,6.86,8.85c1.29,4.31-4.35,4.2-7,3.52-4.84-1.26-2.42-8,1.22-3,.61.83,2,0,1.4-.82-2.15-2.93-6.71-4.9-7.12.35s8.36,7.24,11.72,4.47C135,15.32,129.91,9,127.18,6.19Z\"></path></g>\n            </svg>\n        </div>\n\n        <div class=\"row\" mbr-class=\"{'pt-5':showTitle||showUnderline}\">\n            <div class=\"col-md-6\">\n                <div class=\"menu-group\">\n                    <h3 class=\"mbr-fonts-style group-title mbr-black align-left\" mbr-theme-style=\"display-2\" data-app-selector=\".group-title\">\n                        Salatvariationen</h3>\n                    <div class=\"row menu-row\" mbr-list mbr-list-grow=\"group1Items\" data-specials=\"{{specialsInGroup1}}\">\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">Empfehlung vom Küchenchef</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\